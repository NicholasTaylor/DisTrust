{

    "manifest_version": 2,
    "name": "NoTrust",
    "version": "1.0",
  
    "description": "Deletes annoying OneTrust banners on sight.",
  
    "icons": {
        "48": "icons/notrust-48.png",
        "96": "icons/notrust-96.png"
    },
  
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["notrust.js"]
      }
    ]
  
  }