{

    "manifest_version": 2,
    "name": "DisTrust",
    "version": "1.2",
  
    "description": "Deletes annoying OneTrust banners on sight.",
  
    "icons": {
        "48": "icons/distrust-48.png",
        "96": "icons/distrust-96.png"
    },
  
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["distrust.js"]
      }
    ]
  
  }